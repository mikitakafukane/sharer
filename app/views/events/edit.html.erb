<%= link_to "カレンダーに戻る", events_path, class: "btn btn-success" %>


<h1>テスト</h1>

<%= form_with model: @event, local: true do |f| %>

  <div class="title">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="description">
    <%= f.label :description%>
    <%= f.text_field :description %>
  </div>

  <div class="time">
    <%= f.label :start_date %>
    <%= f.datetime_select :start_date %>
  </div>

  <div class="time">
    <%= f.label :end_date %>
    <%= f.datetime_select :end_date %>
  </div>

  <div class="submit">
    <%= f.submit %>
  </div>

<% end %>